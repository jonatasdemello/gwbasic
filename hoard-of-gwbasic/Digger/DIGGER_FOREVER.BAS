            LINE (s(1), s(0))-(s(1) + 29, s(0) + 29), 0, BF
            f5(9) = 99: EXIT DO
       CASE 15:
            shooting% = 0: shoot% = 0: bullets% = bullets% - 1: points = points + 60
            LINE (s(1), s(0))-(s(1) + 29, s(0) + 29), 0, BF
            f6(9) = 99: EXIT DO
